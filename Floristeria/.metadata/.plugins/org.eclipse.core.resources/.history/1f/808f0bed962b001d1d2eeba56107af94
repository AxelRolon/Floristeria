package Productos;

public class art_Flores extends Art_Factory {
				
	protected String color;
	
	public art_Flores(String nom_art, double precio, String color) {
		super(nom_art, precio);
		this.color=color;
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}
	
}
